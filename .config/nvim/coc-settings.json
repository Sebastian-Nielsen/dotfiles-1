{
  "list.insertMappings": {
    "<c-t>": "action:tabe",
    "<c-v>": "action:vsplit",
    "<c-s>": "action:split",
    "<c-l>": "do:toggleMode",
    "<c-p>": "action:preview",
    "<c-x>": "do:exit"
  },
  "list.normalMappings": {
    "<c-t>": "action:tabe",
    "<c-v>": "action:vsplit",
    "<c-s>": "action:split",
    "<c-l>": "do:toggleMode",
    "<c-p>": "action:preview",
    "<c-x>": "do:exit"
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "json",
    "javascript",
    "markdown",
    "python",
    "typescriptreact",
    "vue"
  ],
  "eslint.packageManager": "yarn",
  "python.formatting.provider": "black"
}
